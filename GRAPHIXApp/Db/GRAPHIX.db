#GRAPHIX 3 Database File


# --- Read Command
record(waveform, "$(DEVICE_NAME):readCMD")
{
	field(DESC, "Send Read Command and Receive Value")
	field(DTYP, "stream")
	field(INP, "@GRAPHIX.proto readCMD $(PORT)")
	field(PINI, "YES")
	field(FTVL, "UCHAR")
	field(NELM, "200")
}


